{
  "name": "mapcontrib-web",
  "version": "2.0.0-alpha.3",
  "private": true,
  "moduleRoots": [
    "./src"
  ],
  "scripts": {
    "start": "cross-env NODE_PATH=src react-scripts-ts start",
    "build": "cross-env NODE_PATH=src react-scripts-ts build",
    "test": "cross-env NODE_PATH=src react-scripts-ts test --env=jsdom",
    "test-ci": "npm test -- --ci --coverage",
    "test-watch": "npm run test -- --coverage --watch",
    "test-format": "prettier --list-different 'src/**/*.{js,jsx,json,css,less}'",
    "lint": "eslint 'src/**/*.{js,jsx}'",
    "eject": "react-scripts-ts eject",
    "pre-commit": "lint-staged"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/helpers/storage.js",
      "!node_modules"
    ],
    "coverageReporters": [
      "json",
      "lcov"
    ]
  },
  "dependencies": {
    "@types/jest": "^23.3.1",
    "@types/jquery": "^3.3.5",
    "@types/lodash": "^4.14.116",
    "@types/node": "^10.5.8",
    "@types/react": "^16.4.9",
    "@types/react-dom": "^16.0.7",
    "@types/react-redux": "^6.0.6",
    "@types/react-router-dom": "^4.3.0",
    "@types/redux": "^3.6.0",
    "@types/redux-thunk": "^2.1.0",
    "@types/shortid": "0.0.29",
    "classnames": "^2.2.5",
    "diacritic": "^0.0.2",
    "expose-loader": "^0.7.5",
    "jquery": "^3.2.1",
    "leaflet-nectarivore": "^0.2.0",
    "leaflet-overpass-layer": "^2.7.0",
    "lodash": "^4.17.5",
    "osm-auth": "^1.0.2",
    "osm-request": "^1.0.4",
    "osm-ui-react": "^0.6.3",
    "osmose-request": "^1.3.0",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-transition-group": "^2.3.0",
    "redux": "^4.0.0",
    "redux-localstorage": "^0.4.1",
    "redux-thunk": "^2.2.0",
    "shortid": "^2.2.8",
    "styled-components": "^3.4.2"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^5.2.0",
    "babel-plugin-styled-components": "^1.5.1",
    "coveralls": "^3.0.1",
    "cross-env": "^5.1.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint-config-prettier": "^2.9.0",
    "jest-styled-components": "^6.0.1",
    "lint-staged": "^7.1.2",
    "prettier": "^1.11.1",
    "react-scripts-ts": "2.17.0",
    "source-map-loader": "^0.2.3",
    "typescript": "^3.0.1"
  }
}
